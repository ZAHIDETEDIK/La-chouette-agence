#redirection https
RewriteEngine on
RewriteCond %{HTTPS} !=on
RewriteRule ^htpps://%{ HTTP_HOST}{%REQUEST_URI } [R=301,L]
#redirection vers WWW
RewriteEngine on
RewriteCond %{HTTP_HOST } !^www\.
RewriteRule^(.*)$ http//{ HTTP_HOST}/$1 [R=301,L]
#cache-control
<IfModule mod_headers.c>
<FilesMatch “\.(ico|jpeg|jpg|png|wepb| css|gz|eot|ttf|woff2|otf)$”>
Header set Cache-Control "max-age=2592000,private"
</FilesMatch>
 <FilesMatch "\\.(css)$">
 Header set Cache-Control "max-age=2592000, public"
<FilesMatch "\.(js)$">
Header set Cache-Control "max-age=2592000, private"
</FilesMatch>
<FilesMatch "\.(html/htm)$">
Header set Cache-Control "max-age= 7200,public"
</IfModule>


<IfModule mod_expires.c>
ExpiresActive On
ExpiresDefault "access plus 7200 seconds"
ExpiresByType image/jpg "access plus 2592000 seconds"
ExpiresByType image/jpeg "access plus 2592000 seconds"
ExpiresByType image/gif "access plus 2592000 seconds"
ExpiresByType image/png "access plus 2592000 seconds"
ExpiresByType image/wepb "access plus 2592000 seconds"
ExpresByType image/svg+xml "access plus 2592000 seconds"
ExpiresByType image/x-icon "access plus 2592000 seconds"
ExpiresByType text/css "access plus 2592000 seconds"
ExpiresByType text/html "access plus 2592000 seconds"
ExpiresByType text/x-javascript "access plus 2592000 seconds"
ExpiresByType application/javascript "access plus 2592000 seconds"
ExpiresByType application/vnd.ms-fontobject "access plus 2592000 seconds"
ExpiresByType font/ttf "access plus 2592000 seconds"
ExpiresByType font/otf "access plus 2592000 seconds"
ExpiresByType font/woff "access plus 2592000 seconds"
ExpiresByType font/woff2 "access plus 2592000 seconds“


</IfModule>